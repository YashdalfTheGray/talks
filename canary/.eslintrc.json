{
  "extends": ["formidable/configurations/es6-react", "plugin:prettier/recommended"],
  "rules": {
    "no-magic-numbers": ["off"],
    "react/jsx-closing-bracket-location": ["off"],
    "react/no-multi-comp": ["off"],
    "react/prefer-es6-class": ["off"]
  },
  "env": {
    "browser": true,
    "node": true
  }
}
